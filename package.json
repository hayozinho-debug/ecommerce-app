{
  "name": "ecommerce-app",
  "version": "1.0.0",
  "description": "A simple eCommerce application built with TypeScript.",
  "main": "src/server.ts",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "dev:server": "ts-node-dev src/server.ts --respawn --transpile-only",
    "dev": "ts-node-dev src/server.ts --respawn --transpile-only",
    "prisma:generate": "prisma generate",
    "migrate": "prisma migrate dev --name init",
    "migrate:deploy": "prisma migrate deploy",
    "seed": "ts-node prisma/seed.ts",
    "db:seed": "prisma db seed",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.31",
    "prisma": "^4.0.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.0.0",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "ecommerce",
    "typescript",
    "nodejs",
    "express"
  ],
  "author": "Your Name",
  "license": "MIT",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
